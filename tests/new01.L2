 (:go
  (:fill_mult
   2 0	;; first is size, second is factor decoded
   ;;make array
   (factor <- rsi)
   (rsi <- 1)
   (call allocate 2)

   ;;initialize counters
   (rdi <- rax)
   (rdi += 8)
   (rcx <- (mem rax 0))
   (rdx <- rcx)
   (r8 <- factor)

   :loop
   (cjump rcx = 0 :done :more)

   :more
   (rsi <- rdx)
   (rsi -= rcx)
   (rsi *= r8)
   (rsi *= 2)
   (rsi += 1)
   ((mem rdi 0) <- rsi)
   (rcx -= 1)
   (rdi += 8)
(goto :loop)

   :done
   (return))

   (:go
	0 0
	(rdi <- 13)
	(rsi <- 7)
	((mem rsp -8) <- :fill_ret)
	(call :fill_mult 2)
	:fill_ret
	(rdi <- rax)
	(call print 1)
	(return)))
